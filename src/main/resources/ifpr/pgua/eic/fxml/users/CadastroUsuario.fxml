<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>


<HBox xmlns="http://javafx.com/javafx" 
      xmlns:fx="http://javafx.com/fxml" 
      prefHeight="720" prefWidth="1280"
      stylesheets="@../../styles/users/CadastroUsuario.css"
      fx:controller="ifpr.pgua.eic.controllers.users.CadastroUsuarioController"
      >

    <VBox prefHeight="720.0" prefWidth="1280.0" alignment="center" spacing="20.0" styleClass="container-form">
        <children>
            <Label fx:id="lbTitle" styleClass="title-form"  />
            <MFXTextField  floatMode="BORDER" floatingText="Nome" prefWidth="400" prefHeight="35" fx:id="tfNome"/>
            <MFXTextField  floatMode="BORDER" floatingText="Sobrenome" prefWidth="400" prefHeight="35" fx:id="tfSobrenome"/>
            <MFXTextField floatMode="BORDER" floatingText="Email a ser cadastrado"  prefWidth="400" prefHeight="35" fx:id="tfEmail"/>
            <MFXButton prefWidth="400.0" text="Cadastrar" styleClass="btn-submit" onAction="#cadastrarUsuario" />
        </children>
    </VBox>

</HBox>